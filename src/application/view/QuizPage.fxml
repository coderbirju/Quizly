<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="viewArea" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.QuizPage_Controller">
   <children>
      <TextField fx:id="Code" layoutX="268.0" layoutY="183.0" promptText="Code" />
      <Label layoutX="99.0" layoutY="185.0" text="Enter the code">
         <font>
            <Font name="Arial Bold" size="18.0" />
         </font>
      </Label>
      <Button fx:id="submit" layoutX="225.0" layoutY="254.0" mnemonicParsing="false" onAction="#submit" style="-fx-background-color: #23578C; -fx-cursor: hand;" text="Submit" textFill="#f8f5f5" />
      <Label fx:id="errorMessage" layoutX="217.0" layoutY="223.0" />
   </children>
</AnchorPane>
